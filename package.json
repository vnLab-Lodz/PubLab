{
  "name": "pub_lab",
  "productName": "PubLab",
  "author": {
    "name": "vnLab",
    "url": "http://vnlab.filmschool.lodz.pl/"
  },
  "version": "1.0.0",
  "description": "Tool aiming to ease the creation and adoption of interactive web publications.",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "lint": "eslint ./src",
    "format": "prettier --write --config .prettierrc ./src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "cross-env NODE_ENV=test jest"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "PubLab"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./config/webpack.main.config.js",
            "renderer": {
              "config": "./config/webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/renderer/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.2",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.10",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.11",
    "@types/react-select": "^3.0.26",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.2.1",
    "electron": "10.1.4",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "jest": "^27.3.1",
    "node-loader": "^1.0.1",
    "prettier": "2.4.1",
    "react-refresh": "^0.9.0",
    "redux-devtools": "^3.7.0",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.4",
    "@reduxjs/toolkit": "^1.4.0",
    "axios": "^0.21.0",
    "child_process": "^1.0.2",
    "dotenv": "^8.2.0",
    "electron-redux": "^1.5.4",
    "electron-squirrel-startup": "^1.0.0",
    "i18next": "^21.3.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.13.0",
    "react-icons": "^4.1.0",
    "react-redux": "^7.2.2",
    "react-select": "^3.1.1",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "svg-url-loader": "^7.1.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2"
  }
}
